{
  "name": "ROEApp",
  "version": "1.0.0",
  "description": "ROE application",
  "author": "Sumeet Agrawal",
  "main": "www/index.js",
  "build": {
    "appId": "org.scott.roeapp",
    "category": "corp.app-category.sales",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "iconUrl": "http://eatodo.s3.amazonaws.com/icon.ico",
      "publish": [
        "bintray"
      ]
    }
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "gulp compile && electron ./www --enable-logging",
    "dist": "build -mw --x64 --ia32"
  },
  "dependencies": {
    "@angular/common": "~2.1.1",
    "@angular/compiler": "~2.1.1",
    "@angular/core": "~2.1.1",
    "@angular/forms": "~2.1.1",
    "@angular/http": "~2.1.1",
    "@angular/platform-browser": "~2.1.1",
    "@angular/platform-browser-dynamic": "~2.1.1",
    "@angular/router": "~3.1.1",
    "@angular/upgrade": "~2.1.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.13",
    "@angular/material": "^2.0.0-alpha.10",
    "@types/node": "^6.0.48",
    "angular-in-memory-web-api": "~0.1.13",
    "core-js": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-typescript": "*",
    "hammerjs": "^2.0.8",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/hammerjs": "^2.0.33",
    "@types/node": "^6.0.45",
    "bower": "^1.3.3",
    "concurrently": "^3.0.0",
    "devtron": "^1.1.2",
    "electron": "^9.1.0",
    "electron-builder": "~5.12.X",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^1.2.0",
    "gulp-sass": "^2.3.2",
    "gulp-tsc": "1.2.5",
    "gulp-util": "^3.0.7",
    "run-sequence": "^1.2.1",
    "shelljs": "0.5.2",
    "typescript": "^2.0.3",
    "typings": "^2.0.0",
    "wiredep": "^4.0.0"
  }
}
